{
    "name": "vscode-json5",
    "displayName": "JSON5 syntax extended",
    "description": "This extension extends the JSON5 syntax highlighting for keys and numbers.",
    "version": "1.0.2",
    "publisher": "dimateos",
    "license": "MIT",
    "engines": {
        "vscode": "^1.3.0"
    },
    "homepage": "https://github.com/dimateos/vscode-json5/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/dimateos/vscode-json5"
    },
    "keywords": [
        "json5",
        "syntax",
        "highlight"
    ],
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "json5",
                "aliases": [
                    "JSON5",
                    "json5"
                ],
                "extensions": [
                    ".json5"
                ],
                "configuration": "./json5.configuration.json",
                "icon": {
                    "dark":  "./icons/json-material.svg",
                    "light":  "./icons/json-material.svg"
                }
            }
        ],
        "grammars": [
            {
                "language": "json5",
                "scopeName": "source.json5",
                "path": "./syntaxes/json5.json"
            }
        ]
    }
}
